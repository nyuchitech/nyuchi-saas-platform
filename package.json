{
  "name": "nyuchi-platform",
  "version": "1.0.0",
  "description": "Nyuchi Platform - SaaS Dashboard for Digital Marketing and SEO Management",
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 4321",
    "build": "astro build",
    "wrangler:dev": "wrangler dev",
    "deploy": "npm run build && wrangler deploy --env production",
    "db:create": "wrangler d1 create nyuchi-dashboard-db",
    "db:migrate": "wrangler d1 migrations apply nyuchi-dashboard-db --local",
    "check": "astro check",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.astro --fix",
    "supabase:login": "npx supabase login",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:types": "npx supabase gen types typescript --local > database/types/supabase.ts",
    "setup:env": "bash scripts/setup-env.sh"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.7",
    "@astrojs/tailwind": "^5.1.5",
    "@astrojs/react": "^3.6.2",
    "@supabase/supabase-js": "^2.46.4",
    "astro": "^5.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "react-hook-form": "^7.55.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss": "^3.4.16",
    "nanoid": "^5.0.4",
    "stripe": "^16.12.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@cloudflare/workers-types": "^4.20241004.0",
    "@types/node": "^22.17.2",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "supabase": "^2.34.3",
    "typescript": "^5.7.2",
    "wrangler": "^4.38.0"
  },
  "overrides": {
    "esbuild": "^0.25.9",
    "@babel/runtime": "^7.26.10"
  },
  "keywords": [
    "astro",
    "saas-dashboard",
    "seo-management",
    "digital-marketing",
    "cloudflare",
    "nyuchi",
    "supabase"
  ],
  "author": "Nyuchi Web Services <hello@nyuchi.com>",
  "license": "GPL-2.0-or-later"
}
